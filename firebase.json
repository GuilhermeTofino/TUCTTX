{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "com-app-treino",
          "appId": "1:928032785050:android:890f79e61b94cefeeb9523",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "com-app-treino",
          "appId": "1:928032785050:ios:08edb049d2fc0930eb9523",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "com-app-treino",
          "configurations": {
            "android": "1:928032785050:android:59f10e566156a38deb9523",
            "ios": "1:928032785050:ios:ca86df22eaa0a3fceb9523"
          }
        }
      }
    }
  },
  "firestore": {
    "rules": "firestore.regras",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "publico",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
